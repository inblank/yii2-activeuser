{
  "name": "inblank/yii2-activeuser",
  "type": "yii2-extension",
  "description": "Yii2 module inblank/yii2-activeuser",
  "keywords": [
    "yii2",
    "module",
    "activeuser"
  ],
  "license": "MIT",
  "homepage": "https://github.com/inblank/yii2-activeuser",
  "authors": [
    {
      "name": "Pavel Aleksandrov",
      "email": "inblank@yandex.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "yiisoft/yii2": "2.0.*",
    "yiisoft/yii2-swiftmailer": "~2.0",
    "roderik/pwgen-php": "~0.1",
    "inblank/yii2-image": "~0.1"
  },
  "require-dev": {
    "codeception/codeception": "^2.1",
    "codeception/specify": "~0.4",
    "codeception/verify": "~0.3"
  },
  "repositories": [
    {
        "type": "composer",
        "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
        "enabled": false
    }
  },
  "autoload": {
    "psr-4": {
      "inblank\\activeuser\\": ""
    }
  },
  "extra": {
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "bootstrap": "inblank\\activeuser\\Bootstrap"
  }
}
